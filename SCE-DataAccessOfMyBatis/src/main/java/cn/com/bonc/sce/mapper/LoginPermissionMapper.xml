<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.bonc.sce.mapper.LoginPermissionMapper">

    <update id="updateLoginPermission">
        UPDATE
        SCE_COMMON_USER
        SET LOGIN_PERMISSION_STATUS = #{loginPermissionStatus}
        WHERE
        APP_ID = #{userId}
    </update>
</mapper>